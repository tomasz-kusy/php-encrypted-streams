{
  "name": "jsq/psr7-stream-encryption",
  "description": "For encrypting and decrypting streams of arbitrary size.",
  "minimum-stability": "stable",
  "keywords": ["psr","psr-7","stream","encryption"],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jonathan Eskew",
      "email": "jonathan@jeskew.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jsq\\EncryptionStreams\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jsq\\EncryptionStreams\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3|^8",
    "ext-openssl": "*",
    "guzzlehttp/psr7": "^1.9|^2.0",
    "psr/http-message": "^1.0|^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  }
